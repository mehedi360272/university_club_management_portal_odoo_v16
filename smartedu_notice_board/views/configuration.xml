<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
<!--    tree view start-->

        <record id="configuration_view_tree" model="ir.ui.view">
            <field name="name">configuration_view_tree</field>
            <field name="model">se.notice.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom" multi_edit="1">
                    <field name="name"/>
                </tree>
            </field>
        </record>


          <record id="open_view_configuration_action" model="ir.actions.act_window">
            <field name="name">Configuration</field>
            <field name="res_model">se.notice.type</field>
            <field name="view_mode">tree,form</field>

          </record>
    </data>

</odoo>
