<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!--    tree view-->
        <record id="uniclub_club_type_view_tree" model="ir.ui.view">
            <field name="name">uniclub_club_type_view_tree</field>
            <field name="model">uniclub.club.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom" string="Type" sample="1">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="action_club_type" model="ir.actions.act_window">
            <field name="name">Club Type</field>
            <field name="res_model">uniclub.club.type</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>
